@model WebMVCR1.Models.PersonRepository
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    <div>
        <h3>List of participants</h3> <p>Persons in total @ViewBag.Count</p>
        <ul>
            @foreach (var person in ViewBag.Persons)
            {
                <li>@person.ToString()</li>
            }
        </ul> 
        <p>@Html.ActionLink("To main", "Index")</p>
        <p>@Html.ActionLink("Look who's already here", "OutputData") </p>
    </div>
}
